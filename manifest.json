{
  "manifest_version": 2,
  "name": "Inbox ToDo Count",
  "description": "Shows the number of items in the Google Inbox",
  "version": "0.1.0",

  "content_scripts": [
    {
      "matches": [
        "*://inbox.google.com/*"
      ],
      "js": [
        "js/inbox.js"
      ],
      "run_at": "document_end"
    }
  ]
}
